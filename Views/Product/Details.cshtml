@model NimaApp.Models.ProductMaster

@{
    ViewBag.Title = "Details";
}

<h1>Product Details</h1>

<div class="container" >
    <table class="table table-dark text-white">
        <tr>
            <th>@Html.LabelFor(m=> m.ProductId)</th>
            <th>@Html.LabelFor(m=> m.ProductName)</th>
        </tr>
        <tr>
            <td>
                @Model.ProductId
            </td>
            <td>
                @Model.ProductName
            </td>
        </tr>
    </table>
    
</div>
